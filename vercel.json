{
  "version": 2,
  "builds": [
    {
      "src": "src/server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/index.ts"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "src/server/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 