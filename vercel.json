{
  "version": 2,
  "builds": [
    {
      "src": "src/server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server/index.ts"
    }
  ],
  "env": {
    "MONGODB_URI": "${MONGODB_URI}",
    "SESSION_SECRET": "${SESSION_SECRET}",
    "NODE_ENV": "production"
  }
} 